            "homepage": "https://www.maltehuebner.de/",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": { "App\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "require": {
        "php": "^8.4",
        "ext-intl": "*",
        "ext-simplexml": "*",
        "doctrine/orm": "^3.4",
        "doctrine/doctrine-bundle": "^3.2",
        "doctrine/doctrine-migrations-bundle": "^4.0",
        "vich/uploader-bundle": "^2.4",
        "liip/imagine-bundle": "^2.0",
        "knplabs/knp-paginator-bundle": "^6.0",
        "emcconville/google-map-polyline-encoding-tool": ">=1.2.1",
        "symfony/monolog-bundle": "^4.0",
        "hwi/oauth-bundle": "dev-master",
        "malenki/slug": "1.0.0",
        "sonata-project/intl-bundle": "^3.0",
        "miljar/php-exif": "^0.6.5",
        "sonata-project/seo-bundle": "^3.0",
        "salavert/time-ago-in-words": "^1.8",
        "emcconville/point-reduction-algorithms": "^1.2",
        "php-http/message": "^1.6",
        "symfony/security-bundle": "^8.0",
        "symfony/apache-pack": "^1.0",
        "symfony/flex": "^2.0",
        "khill/php-duration": "^1.0",
        "sabre/vobject": "^4.0",
        "oneup/flysystem-bundle": "^4.12.2",
        "flagception/flagception-bundle": "^6.0",
        "ext-fileinfo": "*",
        "ext-xmlwriter": "*",
        "symfony/webpack-encore-bundle": "^2.4",
        "ext-json": "*",
        "ext-zip": "*",
        "doctrine/annotations": "^2.0",
        "league/commonmark": "^2.6",
        "nesbot/carbon": "^3.0",
        "iamstuartwilson/strava": "^1.4",
        "embed/embed": "^4.1",
        "twig/extra-bundle": "^3.10",
        "nyholm/psr7": "^1.3",
        "knplabs/knp-menu-bundle": "^3.0",
        "maltehuebner/ordered-entities-bundle": "^0.3",
        "symfony/doctrine-bridge": "^8.0",
        "symfony/framework-bundle": "^8.0",
        "symfony/dotenv": "^8.0",
        "jaybizzle/crawler-detect": "^1.2",
        "nelmio/api-doc-bundle": "^5.9",
        "symfony/asset": "^8.0",
        "yohang/calendr": "^4.0",
        "presta/sitemap-bundle": "^4.0",
        "symfony/http-client": "^8.0",
        "symfony/twig-bridge": "^8.0",
        "symfony/mailer": "^8.0",
        "symfony/cache": "^8.0",
        "symfony/notifier": "^8.0",
        "symfony/twig-bundle": "^8.0",
        "twig/cssinliner-extra": "^3.10",
        "twig/inky-extra": "^3.10",
        "symfony/serializer": "^8.0",
        "symfony/runtime": "^8.0",
        "symfony/stimulus-bundle": "^2.25",
        "phpdocumentor/reflection-docblock": "^5.6",
        "phpstan/phpdoc-parser": "^2.1",
        "symfony/property-access": "^8.0",
        "symfony/property-info": "^8.0",
        "symfony/messenger": "^8.0",
        "beberlei/doctrineextensions": "^1.5",
        "sibyx/phpgpx": "^1.3",
        "twig/intl-extra": "^3.23",
        "maltehuebner/dataquery-bundle": "0.9.1"
    },
    "require-dev": {
        "symfony/browser-kit": "^8.0",
        "symfony/phpunit-bridge": "^8.0",
        "phpunit/phpunit": "^11.0",
        "symfony/maker-bundle": "^1.11",
        "symfony/stopwatch": "^8.0",
        "symfony/web-profiler-bundle": "^8.0",
        "rector/rector": "^2.0",
        "doctrine/doctrine-fixtures-bundle": "^4.0"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "scripts": {
        "post-install-cmd": [
        ],
        "post-update-cmd": [
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "test:db:reset": [
            "php bin/console --env=test doctrine:schema:drop --force --full-database",
            "php bin/console --env=test doctrine:schema:create",
            "php bin/console --env=test doctrine:fixtures:load --no-interaction"
        ],
        "test:run": [
            "php vendor/bin/phpunit"
        ],
        "test:api": [
            "php vendor/bin/phpunit --testsuite 'API Tests'"
        ],
        "test": [
            "@test:db:reset",
            "@test:run"
        ],
        "tests": [
            "@test"
        ]
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "php-http/discovery": true,
            "symfony/runtime": true
        },
        "audit": {
            "abandoned": "report",
            "block-insecure": false
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "8.0.*"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/maltehuebner/StravaPHP.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/maltehuebner/time-ago-in-words.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
