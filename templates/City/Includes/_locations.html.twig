{% if locations|length > 0 %}
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">
                Treffpunkte
            </h3>
        </div>
        <div class="panel-body">
            <ul>
                {% for location in locations %}
                    <li>
                        <a href="{{ object_path(location) }}">
                            {{ location.title }}
                        </a>
                    </li>
                {% endfor %}
            </ul>
        </div>
    </div>
{% endif %}